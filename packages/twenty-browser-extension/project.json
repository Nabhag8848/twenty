{
  "name": "twenty-browser-extension",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/twenty-browser-extension/src",
  "projectType": "application",
  "tags": ["scope:browser-extension"],
  "targets": {
    "start": {
      "executor": "nx:run-commands",
      "options": {
        "command": "plasmo dev --build-path=dist",
        "cwd": "packages/twenty-browser-extension",
        "color": true
      },
      "configurations": {
        "development": {
          "command": "plasmo dev --build-path=dist"
        }
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "plasmo build --build-path=dist",
        "cwd": "packages/twenty-browser-extension",
        "color": true
      },
      "configurations": {
        "production": {
          "command": "plasmo build --build-path=dist --zip"
        }
      }
    },
    "package": {
      "executor": "nx:run-commands",
      "options": {
        "command": "plasmo package --build-path=dist",
        "cwd": "packages/twenty-browser-extension",
        "color": true
      },
      "configurations": {
        "production": {
          "command": "plasmo package --build-path=dist"
        }
      }
    }
  }
}
